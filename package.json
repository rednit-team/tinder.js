{
  "name": "tinderjs",
  "version": "1.0.0",
  "description": "Javascript Wrapper for the Tinder API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "typedoc": "^0.22.10"
  },
  "scripts": {
    "lint-staged": "npm run fix",
    "fix": "eslint --cache --fix src && prettier --write --ignore-unknown src",
    "lint": "eslint src --ext .ts && prettier --check --ignore-unknown src",
    "build": "tsc",
    "generate-docs": "npx typedoc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "typescript": "^4.5.4"
  }
}
